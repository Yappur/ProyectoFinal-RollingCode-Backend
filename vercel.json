{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/usuarios/(.*)",
      "dest": "app.js"
    },
    {
      "src": "/clases/(.*)",
      "dest": "app.js"
    },
    {
      "src": "/turnos/(.*)",
      "dest": "app.js"
    },
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ]
}
